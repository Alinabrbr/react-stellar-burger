a>\n    )\n}\n\nexport default NavBar;\n\nNavBar.propTypes = {\n    children: PropTypes.node,\n}","/Users/alina/practicum/stellar-burgers/src/components/Container/Container.jsx",[],[],"/Users/alina/practicum/stellar-burgers/src/components/Card/Card.jsx",[],[],"/Users/alina/practicum/stellar-burgers/src/components/Price/Price.jsx",[],[],"/Users/alina/practicum/stellar-burgers/src/components/Burger-constructor/Burger-Constructor.jsx",[],[],"/Users/alina/practicum/stellar-burgers/src/components/Modal/Modal.jsx",["175"],[],"import React, {useEffect} from \"react\";\nimport styles from \"../Modal/Modal.module.css\";\nimport clsx from \"clsx\";\nimport {createPortal} from \"react-dom\";\nimport {CloseIcon} from \"@ya.praktikum/react-developer-burger-ui-components\";\nimport ModalOverlay from \"../Modal-overlay/Modal-overlay\";\nimport PropTypes from \"prop-types\";\n\nexport default function Modal(props) {\n    const closeModal = props.closeModal;\n\n    const modalElement = document.getElementById('modal');\n\n    useEffect(() => {\n        function onEsc(evt) {\n            if (evt.code === \"Escape\") {\n                closeModal();\n            }\n        }\n\n        document.addEventListener('keydown', onEsc);\n        return () => document.removeEventListener('keydown', onEsc)\n    }, []);\n\n    return createPortal(\n        (\n            <>\n                <div className={clsx(styles.modal, 'pt-10 pr-10 pl-10 pb-15')}>\n                    {props.children}\n                    <div className={styles.closeIcon} onClick={closeModal}>\n                        <CloseIcon type=\"primary\"/>\n                    </div>\n                </div>\n                <ModalOverlay closeModal={closeModal}/>\n            </>\n        ),\n        modalElement\n    )\n}\n\nModal.propTypes = {\n    closeModal: PropTypes.func,\n    children: PropTypes.element\n}","/Users/alina/practicum/stellar-burgers/src/components/Modal-overlay/Modal-overlay.jsx",[],[],"/Users/alina/practicum/