ddIngredient(card));\n        }\n    });\n\n    const deleteIngredient = (card) => {\n        dispatch(removeIngredient(card));\n    }\n\n    const clearStoreBurgerConstructor = () => {\n        dispatch(clearStore());\n    }\n\n    return (\n        <>\n            <section className={clsx(styles.burgerConstructor, 'mt-25')} ref={dropRef}\n                     onDragOver={(evt) => evt.preventDefault()}>\n                <div className={clsx(styles.burgerIngredientsContainer, 'pl-8')}>\n                    <div className='mr-4 ml-4'>\n                        {\n                            bun && (\n                                <ConstructorElement text={`${bun.name} (верх)`} isLocked='isLocked' type='top' thumbnail={bun.image} price={bun.price}/>\n                            )\n                        }\n                    </div>\n                </div>\n\n                <div className={clsx(styles.burgerIngredientsContainerScroll, 'mb-4')}>\n                    <div className='mr-4'>\n                        {cards.map((card, index) => (\n                            (card.type === \"main\" || card.type ==='sauce') && <BurgerConstructor card={card} index={index} key={card.ingredientId} handleClose={() => deleteIngredient(card)}/>\n                        ))}\n                    </div>\n                </div>\n\n                <div className={clsx(styles.burgerIngredientsContainer, 'pl-8')}>\n                    <div className='mr-4 ml-4'>\n                        {\n                            bun && (\n                                <ConstructorElement text={`${bun.name} (низ)`} isLocked='isLocked' type='bottom' thumbnail={bun.image} price={bun.price}/>\n                            )\n                        }\n                    </div>\n                </div>\n\n                <div className={clsx(styles.priceContainer, 'mt-10')}>\n                    <Price priceSize={\"medium\"} price={totalPrice}/>\n                    <Button onClick={() => {\n                        openModal();\n                        dispatch(fetchOrderResult({ingredients: [...cards.map((ingredient) => ingredient._id), bun._id]}));\n                    }}\n                        disabled={cards.length === 0 || !cards.find((item) => item.type === \"bun\")}\n                            htmlType=\"button\" type=\"primary\" size=\"large\">\n                        Оформить заказ\n                    </Button>\n                </div>\n            </section>\n\n            {modalOrderState && <Modal closeModal={() => {closeModal(); clearStoreBurgerConstructor()}}><OrderDetails/></Modal>}\n        </>\n    )\n}","/Users/alina/practicum/stellar-burgers/src/utils/API.js",[],[],"/Users/alina/practicum/stellar-burgers/src/utils/Api.js",[],[],"/Users/alina/practicum/stellar-burgers/src/components/SectionBurgerIngredients/SectionBurgerIngredients.jsx",[],[],"/Users/alina/practicum/stellar-burgers/src/services/cardsSlice.js",[],[],"/Users/alina/practicum/stellar-burgers/src/services/ingredientsInfoSlice.js",[],[],"/Users/alina/practicum/stellar-burgers/src/services/getModalInfoSelector.js",[],[],"/Users/alina/practicum/stellar-burgers/src/services/getModalOrderSelector.js",[],[],"/Users/alina/practicum/stellar-burgers/src/services/orderSlice.js",[],[],"/Users/alina/practicum/stellar-burgers/src/services/constructorSlice.js",[],[],"/Users/alina/practicum/stellar-burgers/src/services/constructorSelector.js",[],[],"/Users/alina/practicum/stellar-burgers/src/utils/burger-price.js",[],[],"/Users/alina/practicum/stellar-burgers/src/services/totalPriceSelector.js",[],[],"/Users/alina/practicum/stellar-burgers/src/services/orderDetailsSlice.js",[],[],"/Users/alina/practicum/stellar-burgers/src/services/cardsSelector.js",[],[],{"ruleId":"177","severity":1,"message":"178","line":6,"column":9,"nodeType":"179","endLine":6,"endColumn":45},{"ruleId":"177","severity":1,"message":"178","line":6,"column":9,"nodeType":"179","endLine":6,"endColumn":45},{"ruleId":"177","sev